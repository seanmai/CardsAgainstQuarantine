ner/Login/Login.js",["86","87","88"],"import React, { Component } from 'react';\nimport './Login.css'\nimport axios from 'axios';\nimport { connect } from 'react-redux';\nimport { Redirect } from 'react-router-dom';\n\nclass Login extends Component {\n\n    state = {\n        mode: 'Login',\n        credentials : {\n            username: '',\n            password: ''\n        },\n        valid: false,\n        auth: false,\n        error: null\n    }\n\n    submitHandler = (event) => {\n        event.preventDefault();\n        // Successful signup\n        const onSuccess = res => {\n            this.setState({auth: res.status})\n        }\n\n        const onFailure = error => {\n            this.setState({error: true})\n        }\n\n        if (this.state.mode === 'Sign up' && this.state.valid){\n            axios.post('http://localhost:4000/users/register', this.state.credentials)\n            .then(onSuccess)\n            .catch(onFailure);\n        }else if (this.state.mode === 'Login'){\n            axios.post('http://localhost:4000/users/login', this.state.credentials)\n            .then(res => this.setState({auth: res.status === 200}))\n            .catch(onFailure);\n        }else {\n            onFailure();\n        }\n    }\n\n    createNewUser = () => {\n        this.setState({ mode: 'Sign up', \n                        error: false})\n    }\n\n    usernameOnChangeHandler = (event) => {\n        this.state.error = false;\n        this.setState(prevState => ({\n            ...prevState,\n            credentials: {\n                ...prevState.credentials,\n                username: event.target.value\n            }\n        }))\n    } \n\n    passwordOnChangeHandler = (event) => {\n        this.state.error = false;\n        this.setState(prevState => ({\n            ...prevState,\n            credentials: {\n                ...prevState.credentials,\n                password: event.target.value\n            }\n        }))\n    } \n\n\n    verifyOnChangeHandler = (event) => {\n        this.state.error = false;\n        this.setState({valid: event.target.value===this.state.credentials.password});\n    } \n\n    componentDidUpdate(){\n        // Authentication steps\n        if (this.state.auth){\n            this.props.setUser({\n                name: this.state.credentials.username\n            })\n            this.props.setAuth(this.state.auth);\n        }\n    }\n\n    render() {\n        let confirmPassword = null;\n        if(this.state.mode === 'Sign up'){\n            confirmPassword = (\n                <div className=\"CredentialField\">\n                    <label>Confirm Password: </label>\n                    <input type=\"password\" onChange={this.verifyOnChangeHandler}></input>     \n                </div>\n            )\n        }\n\n        let error_message = null;\n        if (this.state.error){\n            if (!this.state.valid && this.state.mode === 'Sign up') {\n                error_message = <label className=\"ErrorMessage\">Passwords does not match</label>\n            }else{\n                error_message = <label className=\"ErrorMessage\"> {this.state.mode === 'Login' ? 'Incorrect username/password':'Username is taken'} </label> \n            }\n        }\n\n        let redirect = <Redirect to={this.props.redirect}/>;\n        return (\n            <div className=\"Container\">\n                {redirect}\n                <label className=\"MainTitle\">Cards Against Humanity</label>\n                <div className=\"Login\">\n                    <label className=\"Title\">{this.state.mode}</label>\n                    <form className=\"LoginForm\" onSubmit={this.submitHandler}>\n                        {error_message}\n                        <div className=\"CredentialField\">\n                            <label>Username: </label>\n                            <input type=\"text\" onChange={this.usernameOnChangeHandler}></input>    \n                        </div>\n\n                        <div className=\"CredentialField\">\n                            <label>Password: </label>\n                            <input type=\"password\" onChange={this.passwordOnChangeHandler}></input>     \n\n                        </div>\n                        {confirmPassword}\n                        <div className=\"ButtonContainer\">\n                            <button className=\"Button\" type=\"submit\">{this.state.mode}</button>\n                            {this.state.mode === 'Login' ? <button className=\"Button\" onClick={this.createNewUser}>New User</button> : null}\n\n                        </div>\n                    </form>\n                </div>\n            </div>\n        );\n    }\n}\n\nfunction mapStateToProps(state) {\n    return {\n        currentUser: state.currentUser,\n        redirect: state.redirect,\n        auth: state.authenticated\n    }\n}\n\nfunction mapDispatchToProps(dispatch){\n    return {\n        setUser: (userObj) => {\n            dispatch({type: \"SET_USER\", payload:userObj})\n        },\n        setRedirect: (path) => {\n            dispatch({type: \"SET_REDIRECT\", payload:path})\n        },\n        setAuth: (auth) => {\n            dispatch({type: \"SET_AUTH\", payload: auth})\n        }\n    }\n}\n\nexport default connect(mapStateToProps,mapDispatchToProps) (Login);\n","/Users/seanmai/Desktop/WebDev/Main/CardsAgainstQuarantine/cards-against-quarantine/src/container/Room/Room.js",["89"],"// This container is the game room\nimport React, { useState } from 'react';\nimport './Room.css'\n\nimport Card from '../../components/Card/Card';\nimport Scoreboard from './Scoreboard'\nimport { connect, useSelector } from 'react-redux';\n\nconst test_cards = [\n    {\n        content: \"stupid\",\n        selected: false\n    },\n    {\n        content: \"dumb\",\n        selected: false\n    },\n    {\n        content: \"ur mom\",\n        selected: false\n    },\n    {\n        content: \"yes\",\n        selected: false\n    },\n    {\n        content: \"test card\",\n        selected: false\n    },\n]\n\nconst played_cards = [\n    {\n        content: \"hehe\",\n        selected: false\n    },\n    {\n        content: \"haha\",\n        selected: false\n    },\n    {\n        content: \"hey\",\n        selected: false\n    },\n    {\n        content: \"other test card\",\n        selected: false\n    },\n]\nconst test_black = \"black card\"\nconst player_type = \"czar\"\n\nconst Room = (props) => {\n    const [cards, setCards] = useState(test_cards)\n    const [selected, setSelected] = useState(\"\")\n    const username = useSelector(state => state.currentUser.name);\n\n    const cardClickHandler = (e) => {\n        if (e.target.value !== \"\") {\n            setSelected(e.target.value)\n            setCards(cards.map(card => card.content === e.target.value ? { ...card, selected: true } : { ...card, selected: false }))\n        }\n    }\n\n    const cardSubmitHandler = (e) => {\n        e.preventDefault()\n        console.log(\"submitting \" + selected)\n    }\n\n    const winSubmitHandler = (e) => {\n        e.preventDefault()\n        console.log(\"winner is \" + selected)\n    }\n    const returnCard = (e) => {\n        setCards(cards.map(card => { return { ...card, selected: false } }))\n        setSelected(\"\")\n    }  \n\n    return (\n        <div className=\"flex-container\">\n            <div className=\"game-area\">\n                <Card\n                    disabled\n                    content={test_black} >\n                </Card>\n                {player_type === 'player' &&\n                    <form onSubmit={cardSubmitHandler}>\n                        <Card\n                            disabled={selected === \"\"}\n                            type=\"button\"\n                            content={selected}\n                            onClick={returnCard} >\n                        </Card>\n                        <button type=\"submit\" disabled={selected === \"\"}>Submit</button>\n                    </form>\n                }\n                {player_type === 'czar' &&\n                    <form onSubmit={winSubmitHandler}>\n                        <div className=\"played-cards\">\n                            {played_cards.map(card => {\n                                return (\n                                    <Card\n                                        key={card.content}\n                                        className={card.content === selected ? \"selected\" : \"\"}\n                                        type=\"button\"\n                                        content={card.selected ? \"\" : card.content}\n                                        onClick={cardClickHandler}>\n                                    </Card>\n                                )\n                            })}\n                        </div>\n                        <button type=\"submit\" disabled={selected === \"\"}>Submit</button>\n                    </form>\n                }\n                <h1>Your Hand</h1>\n                <div className='cardbar'>\n                    {cards.map(card => {\n                        return (\n                            <Card\n                                disabled={player_type === 'czar'}\n                                key={card.content}\n                                content={card.selected ? \"\" : card.content}\n                                onClick={cardClickHandler}>\n                            </Card>\n                        )\n                    })}\n                </div>\n            </div>\n            <div className=\"game-extras\">\n                <Scoreboard />\n                <div>\n                    <h1>\n                        Chat\n                    </h1>\n                </div>\n            </div>\n        </div>\n    )\n}\n\nfunction mapStateToProps(state) {\n    return {\n        currentUser: state.currentUser,\n        redirect: state.redirect,\n        auth: state.authenticated\n    }\n}\n\nexport default connect(mapStateToProps) (Room);\n","/Users/seanmai/Desktop/WebDev/Main/CardsAgainstQuarantine/cards-against-quarantine/src/container/AdminPage/AdminPage.js",[],"/Users/seanmai/Desktop/WebDev/Main/CardsAgainstQuarantine/cards-against-quarantine/src/container/CreateRoom/CreateRoom.js",[],"/Users/seanmai/Desktop/WebDev/Main/CardsAgainstQuarantine/cards-against-quarantine/src/container/Room/Scoreboard.js",[],"/Users/seanmai/Desktop/WebDev/Main/CardsAgainstQuarantine/cards-against-quarantine/src/container/HelpModal/HelpModal.js",["90","91"],"\n\nimport React, { Component } from 'react';\nimport './HelpModal.css'\n\nexport default class HelpModal extends Component {\n    constructor(props){\n        super(props);\n        \n        this.state = {\n            display_role: \"Player\"\n        }\n    }\n\n    viewRole = (role) => {\n        this.setState({display_role: role})\n    }\n\n    render() {\n\n        let displayModal = this.props.showModal ? \"modal display-block\" : \"modal display-none\";\n        let role_description = null;\n\n        if (this.state.display_role === \"Player\"){\n            role_description = [\n                <h2 key=\"Role\">Player</h2>,\n                <h2 key=\"Desc\"></h2>\n            ]\n        }else {\n            role_description = [\n                <h2 key=\"Role\">Czar</h2>,\n                <h2 key=\"Desc\"></h2>\n            ]\n        }\n\n        return (\n            // TODO Design Modal, fill up the text description\n            <div className={displayModal}>\n                <div className=\"modal-main\">\n                    <div>\n                        <button className=\"role_button\" onClick={() => {this.viewRole(\"Player\")}}>Player</button>\n                        <button className=\"role_button\" onClick={() => {this.viewRole(\"Czar\")}}>Card Czar</button>\n                    </div>\n                    {role_description}\n                    <div className=\"closeBox\">\n                        <button onClick={this.props.closeModal}>Close</button>\n                    </div>\n                </div>\n            </div>\n\n        );\n    }\n}\n\n","/Users/seanmai/Desktop/WebDev/Main/CardsAgainstQuarantine/cards-against-quarantine/src/components/Card/Card.js",[],"/Users/seanmai/Desktop/WebDev/Main/CardsAgainstQuarantine/cards-against-quarantine/src/components/CardsList/CardsList.js",[],"/Users/seanmai/Desktop/WebDev/Main/CardsAgainstQuarantine/cards-against-quarantine/src/container/CreateCard/CreateCard.js",[],"/Users/seanmai/Desktop/WebDev/Main/CardsAgainstQuarantine/cards-against-quarantine/src/container/CreateCategory/CreateCategory.js",[],"/Users/seanmai/Desktop/WebDev/Main/CardsAgainstQuarantine/cards-against-quarantine/src/components/CategoryList/CategoryList.js",[],"/Users/seanmai/Desktop/WebDev/Main/CardsAgainstQuarantine/cards-against-quarantine/src/components/EditCard/EditCard.js",[],"/Users/seanmai/Desktop/WebDev/Main/CardsAgainstQuarantine/cards-against-quarantine/src/components/EditCategory/EditCategory.js",[],"/Users/seanmai/Desktop/WebDev/Main/CardsAgainstQuarantine/cards-against-quarantine/src/components/Chat/Chat.js",[],{"ruleId":"92","replacedBy":"93"},{"ruleId":"94","replacedBy":"95"},{"ruleId":"96","severity":1,"message":"97","line":1,"column":8,"nodeType":"98","messageId":"99","endLine":1,"endColumn":12},{"ruleId":"100","severity":1,"message":"101","line":50,"column":9,"nodeType":"102","endLine":50,"endColumn":19},{"ruleId":"100","severity":1,"message":"101","line":61,"column":9,"nodeType":"102","endLine":61,"endColumn":19},{"ruleId":"100","severity":1,"message":"101","line":73,"column":9,"nodeType":"102","endLine":73,"endColumn":19},{"ruleId":"96","severity":1,"message":"103","line":56,"column":11,"nodeType":"98","messageId":"99","endLine":56,"endColumn":19},{"ruleId":"104","severity":1,"message":"105","line":27,"column":17,"nodeType":"106","endLine":27,"endColumn":32},{"ruleId":"104","severity":1,"mess